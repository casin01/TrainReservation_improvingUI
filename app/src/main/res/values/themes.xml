<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ReserveT" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#3f51b5</item>
        <item name="colorPrimaryVariant">#002984</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#ff4081</item>
        <item name="colorSecondaryVariant">#f50057</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->

        <!--windowLightStatusBar 상태바 색상 true면 하얀색-->

    </style>

    <style name="datepicker" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/primaryvariant</item>
        <item name="colorAccent">@color/primary</item>
    </style>

    <style name="b11Text" parent="TextAppearance.AppCompat">
        <item name="android:textSize">11pt</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="b13Text" parent="TextAppearance.AppCompat">
        <item name="android:textSize">13pt</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="a13Text" parent="TextAppearance.AppCompat">
        <item name="android:textSize">13pt</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">@color/active</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="b14Text" parent="TextAppearance.AppCompat">
        <item name="android:textSize">14pt</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="bb14Text" parent="TextAppearance.AppCompat">
        <item name="android:textSize">14pt</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="title18Text" parent="TextAppearance.AppCompat">
        <item name="android:textSize">18pt</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="tabText" parent="TextAppearance.AppCompat">
        <item name="android:textSize">12pt</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="numText" parent="TextAppearance.AppCompat">
        <item name="android:textSize">15pt</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="nonText" parent="TextAppearance.AppCompat">
        <item name="android:textSize">9pt</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">#737373</item>
    </style>

    <style name="btnText" parent="TextAppearance.AppCompat">
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="animguideTxt" parent="TextAppearance.AppCompat">
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">@color/primaryvariant</item>
        <item name="android:textSize">11pt</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="n11Text" parent="TextAppearance.AppCompat">
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textSize">11pt</item>
        <item name="android:textColor">#4A4A4A</item>
    </style>

    <style name="a11Text" parent="TextAppearance.AppCompat">
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">@color/active</item>
        <item name="android:textSize">11pt</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="dialogposText" parent="TextAppearance.AppCompat">
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">@color/secondary</item>
        <item name="android:textSize">11pt</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Theme.ReserveT.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.ReserveT.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.ReserveT.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>